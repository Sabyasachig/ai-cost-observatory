# Environment variables for AI Cost Observatory

# SDK Configuration
AI_OBSERVER_ENDPOINT=http://localhost:8000
AI_OBSERVER_API_KEY=
AI_OBSERVER_ENABLED=true
AI_OBSERVER_TIMEOUT=5

# Server Configuration
DATABASE_URL=sqlite:///./ai_cost_observatory.db
# For PostgreSQL: postgresql://user:password@localhost:5432/ai_cost_observatory
CORS_ORIGINS=*
SQL_ECHO=false

# Dashboard Configuration
API_URL=http://localhost:8000

# OpenAI Configuration (for examples)
OPENAI_API_KEY=your-openai-key-here

# Anthropic Configuration (for examples)
ANTHROPIC_API_KEY=your-anthropic-key-here
